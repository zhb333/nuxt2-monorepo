{
  "name": "nuxt2-monorepo",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "resolutions": {
    "typescript": "4"
  },
  "scripts": {
    "modules:dev": "pnpm --filter modules run watch",
    "modules:build": "pnpm --filter modules run build",
    "app1:dev": "pnpm --filter app1 run dev",
    "app2:dev": "pnpm --filter app2 run dev",
    "lint": "eslint --ext .js,.ts,.vue,.cjs,.mjs --ignore-path .gitignore .",
    "format": "prettier --write --ignore-unknown --ignore-path .prettierignore ."
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.17.0",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vue/composition-api": "^1.7.0",
    "@vue/runtime-dom": "^3.2.23",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "unplugin-vue2-script-setup": "^0.11.3",
    "vue": "2.7.10",
    "webpack": "4.46.0"
  }
}
